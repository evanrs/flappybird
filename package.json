{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A React starter kit using Reflux and Express with hotloading.",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development DEBUG='*' supervisor --harmony --ignore public/build/,client -e md,js,es6,jsx server & npm run hotload",
    "debug": "node-inspector & NODE_ENV=development DEBUG='*' supervisor --debug --harmony --ignore public/build/,client -e md,js,es6,jsx server & npm run hotload",
    "build": "NODE_ENV=production webpack -p --progress --colors",
    "hotload": "NODE_ENV=development DEBUG=webpack:* node --harmony server/hotload",
    "start": "node --harmony server",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evanrs/react-starter.git"
  },
  "author": "Evan Schneider",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.0.12",
    "bluebird": "^2.9.24",
    "body-parser": "^1.12.2",
    "compression": "^1.4.3",
    "config": "^1.12.0",
    "cookie-parser": "^1.3.4",
    "csurf": "^1.8.0",
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "hjson": "^1.6.3",
    "lodash": "^3.6.0",
    "node-jsx": "^0.12.4",
    "passport": "^0.2.1",
    "react": "^0.13.1",
    "react-analytics": "^1.0.3",
    "react-router": "^0.13.2",
    "react-tap-event-plugin": "^0.1.6",
    "redis": "^0.12.1",
    "reflux": "^0.2.7",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.2.0",
    "setimmediate2": "^1.1.4",
    "superagent": "^1.1.0",
    "supervisor": "^0.6.0",
    "tfstyleguide": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.0.12",
    "chai": "^2.2.0",
    "chai-as-promised": "^4.3.0",
    "css-loader": "^0.10.1",
    "extract-text-webpack-plugin": "^0.5.0",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.13.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.4",
    "node-notifier": "^4.1.2",
    "protractor": "^2.0.0",
    "react-hot-loader": "^1.2.4",
    "react-tools": "*",
    "style-loader": "*",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  },
  "bugs": {
